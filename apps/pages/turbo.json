{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"dev": {},
		"check:biome": {
			"inputs": ["src/**/*.ts", "types/**/*.d.ts"],
			"outputs": ["src/**/*.ts", "types/**/*.d.ts"],
			"outputLogs": "errors-only"
		},
		"check:types": {
			"inputs": ["src/**/*.ts", "types/**/*.d.ts"],
			"outputs": ["src/**/*.ts", "types/**/*.d.ts"],
			"outputLogs": "errors-only"
		},
		"check": {
			"dependsOn": ["check:biome", "check:types"]
		}
	}
}
